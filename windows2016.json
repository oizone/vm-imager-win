{
  "builders": [
    {
        "type": "vsphere-iso",
  
        "vcenter_server": "{{user `vsphere-server`}}",
        "username": "{{user `vsphere-user`}}",
        "password": "{{user `vsphere-password`}}",
        "insecure_connection": "true",
  
        "datacenter": "{{user `vsphere-datacenter`}}",
        "cluster": "{{user `vsphere-cluster`}}",
        "datastore": "{{user `vsphere-datastore`}}",
        "folder": "{{user `vsphere-folder`}}",
  
        "communicator": "none",
        "shutdown_timeout": "30m",
  
        "convert_to_template": "false",
	"export": {
	    "force": true,
	    "output_directory": "{{user `vsphere-export-folder`}}/{{user `vm-name`}}/"
	},

        "vm_name": "{{user `vm-name`}}",
        "guest_os_type": "windows9Server64Guest",
  
        "CPUs": "{{user `vm-cpu-num`}}",
        "cpu_cores": "{{user `vm-cpu-cores`}}",
        "CPU_hot_plug": true,
        "RAM": "{{user `vm-mem-size`}}",
        "RAM_reserve_all": false,
        "RAM_hot_plug": true,
        "firmware": "efi",
        "boot_command": ["<spacebar>"],
        "boot_wait": "{{user `boot_wait`}}",
        "cdrom_type": "sata",
        "remove_cdrom": true,
  
        "storage": [
          {
            "disk_size": "{{user `os-disk-size`}}",
            "disk_thin_provisioned": "{{user `disk-thin-provision`}}"
          }
        ],
        "disk_controller_type": "lsilogic-sas",
          
        "network_adapters": [
          {
            "network": "{{user `vsphere-network`}}",
            "network_card": "vmxnet3"
          }
        ],

        "iso_checksum": "md5:{{user `os_iso_checksum`}}",
        "iso_url": "{{user `os_iso_path`}}",
  
        "cd_files": [
            "setup64.exe",
            "autounattend/2016/{{user `vm-name`}}/autounattend.xml",
            "scripts/enable-rdp.cmd",
            "scripts/enable-winrm.ps1",
            "scripts/install-vm-tools.cmd"
        ],
        "cd_label": "InstallMedia"
    }
  ]
  
}
